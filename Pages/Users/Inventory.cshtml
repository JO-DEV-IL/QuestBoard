@page
@model QuestBoard.Pages.InventoryModel
@{

}

<table class="table table-striped m-auto table-hover">
	<tbody>
		@foreach (var item in Model.listUsersItems)
		{
			<tr>
				<td>@item.name</td>
				<td>@item.quantity</td>
				<td>@item.rarity</td>
				<td><img style="height:30px" src="~/images/@item.image" alt="alt" /></td>
				<td><a class="btn btn-warning btn-sm" href="/ViewItem/Show?item=@item.name">View</a></td>
			</tr>
		}
	</tbody>
</table>