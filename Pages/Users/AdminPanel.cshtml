@page
@model QuestBoard.Pages.Users.AdminPanelModel
@{
	@if (HttpContext.Session.GetString("isAdmin") != null && HttpContext.Session.GetString("userActive") != null)
	{
		<div>
			<h2>List of Active Adventurers</h2>
			<table class="table">
				<thead>
					<tr>
						<th>ID</th>
						<th>Username</th>
						<th>First Name</th>
						<th>Last Name</th>
						<th>Age</th>
						<th>Class</th>
						<th>Level</th>
					</tr>
				</thead>
				<tbody>
					@foreach (var item in Model.listUsers)
					{
						<tr>
							<td>@item.userID</td>
							<td>@item.userName</td>
							<td>@item.firstName</td>
							<td>@item.lastName</td>
							<td>@item.age</td>
							<td>@item.class_specialty</td>
							<td>@item.level</td>
						</tr>
					}
				</tbody>
			</table>
		</div>
	}
}